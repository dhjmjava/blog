<?xml version="1.0" encoding="UTF-8" ?>  <!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.blog.mapper.BlogTypesMapper">  <select id="queryTypes" resultType="com.blog.entity.BlogTypes">     select * from t_blog_types  </select>    <select id="getTypeById" resultType="com.blog.entity.BlogTypes" parameterType="long">     select * from t_blog_types where id=#{id}  </select>    <update id="updateType" parameterType="com.blog.entity.BlogTypes">      update t_blog_types set name=#{name} where id=#{id}   </update>    <insert id="saveType" parameterType="com.blog.entity.BlogTypes">      insert into t_blog_types(name,is_use) values(#{name},#{is_use})   </insert>    </mapper>  