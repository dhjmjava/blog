<?xml version="1.0" encoding="UTF-8" ?>  <!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.blog.mapper.LinkMapper">	<select id="queryAllLink" resultType="com.blog.entity.Link">	   select * from t_link	</select>		<delete id="deleteLink" parameterType="long">	   delete from t_link where id = #{id}	</delete>	<insert id="saveLink" parameterType="com.blog.entity.Link">	   insert into t_link(`name`,`url`,`is_use`,`order`) values(#{name},#{url},#{is_use},#{order})	</insert>		<select id="getLinkById" resultType="com.blog.entity.Link" parameterType="long">	    select * from t_link where id = #{id}	</select>		<update id="updateLink" parameterType="com.blog.entity.Link">	    update t_link set `name`=#{name} ,url=#{url} ,is_use=#{is_use},`order` =#{order} where id=#{id}	</update>	</mapper>  